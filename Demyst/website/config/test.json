{
  "app": {
    "port": "3000"
  },
  "remoteServices": {
    "myobApiUrl": "--test--myobApiUrl--",
    "xeroApiUrl": "--test--xeroApiUrl--",
    "decisionEngineApiUrl": "--test--decisionEngineApiUrl--"
  },
  "mongodb": {
    "url": "--test--mongodbUrl--"
  },
  "auth": {
    "salt": "salt",
    "userSaltLength": 20,
    "iterations": 1,
    "hashLength": 64,
    "hashAlgorithm": "sha512",
    "github": {
